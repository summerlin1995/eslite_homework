<h1>Admin::Products#index</h1>
<p>Find me in app/views/admin/products/index.html.erb</p>

<% @products.each do |product| %>
  <div>
    <h1><%= product.name %></h1>
    <%= link_to  "Edit", edit_admin_product_path(product) %>
    <%= link_to  "Show", admin_product_path(product) %>
    <%= link_to  "Photos", admin_product_product_photos_path(product) %>
  </div>
<% end %>
<%= link_to  "Add a product", new_admin_product_path %>
